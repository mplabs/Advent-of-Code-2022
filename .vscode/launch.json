      "type": "node",
      "request": "launch",
      "name": "Build Project",
      "program": "${file}",
      "preLaunchTask": "npm: build",
      "sourceMaps": true,
      "smartStep": true,
      "internalConsoleOptions": "openOnSessionStart",
      "outFiles": [
          "${workspaceFolder}/dist/**/*.js"
      ]
  }

